  <body>
    <div class="book"></div>
    <div class="name">
    <h1>Karel&nbsp;Daněk</h1>
    </div>
    <div class="foto">
    <div>
    <img src="/img/foto.jpg" alt="Karel Daněk" />
    </div>
    </div>
    <div class="container">
      {# <div class="top"><img src="/img/kniha960.png" class="book" alt="srdce" /></div> #}
      <div id="root" class="noscript" data-categories='{{ categoriesFiltered|json_encode|base64_encode|raw }}' data-content='{{ contents|json_encode|base64_encode|raw }}' >
        <div class="sub" id="sub">
          <div id="menu">
            {% for categories in categoriesFiltered %}
              <a href="/archiv/{{ categories[1] }}">{{ categories[1] }}</a>
            {% endfor %}
          </div>
          {% for content in contents %}
            <div class="item">
              <div class="text">
                <h2 id="{{ content.id }}">{{ content.title }}</h2>
                {% apply markdown_to_html %} {{ content.text }} {% endapply %}
                {% if content.author %}
                  <b>Autor:</b> {{ content.author }}
                {% endif %}
                {% if content.source %}
                  <br /><b>Zdroj:</b> {{ content.source }}
                {% endif %}
                <br /><b>Vložil:</b> {{ content.createdBy }} {{ content.created }}
                {% if content.updated %}
                  <br />Naposled upravil: {{ content.updatedBy }} {{ content.updated }}
                {% endif %}
              </div>
              <hr />
              <div class="comments">
                {% for comment in content.comments %}
                  <div class="comment">
                    <p>
                      {{ comment.comment }} <br />napsal {{ comment.by }} {{ comment.at }}
                    </p>
                  </div>
                {% endfor %}
              </div>
            </div>
          {% endfor %}
        </div>
